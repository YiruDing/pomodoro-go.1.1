    "@react-icons/all-files": "^4.1.0",
    "apexcharts": "^3.26.3",
    "axios": "^0.21.4",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dayjs": "^1.10.5",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "faker": "^5.5.3",
    "fs": "^0.0.1-security",
    "generate-password": "^1.6.0",
    "google-auth-library": "^7.1.0",
    "history": "^4.9.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-simple": "^0.5.6",
    "morgan": "^1.9.1",
    "nodemailer": "^6.6.3",
